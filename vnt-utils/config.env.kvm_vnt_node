# KVM VNT network node configuration

#--------------------------------------------------------------------------------------
# KVM_VNT_NODE_NAME: The name of the VNT network node.
unset KVM_VNT_NODE_NAME
: ${KVM_VNT_NODE_NAME:=kvm_node}
#--------------------------------------------------------------------------------------

#--------------------------------------------------------------------------------------
# KVM_VNT_HOST_RAM: The RAM (GB) of the KVM VNT VM.
unset KVM_VNT_NODE_RAM
: ${KVM_VNT_NODE_RAM:=1024}
#--------------------------------------------------------------------------------------

#--------------------------------------------------------------------------------------
# KVM_VNT_HOST_SIZE: The size (GB) of the KVM VNT VM.
unset KVM_VNT_NODE_SIZE
: ${KVM_VNT_NODE_SIZE:=5}
#--------------------------------------------------------------------------------------







